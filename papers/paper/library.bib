  title = {Water Borrowing Is Consistently Practiced Globally and Is Associated with Water-Related System Failures across Diverse Environments},
  shorttitle = {Borrowing},
  author = {Rosinger, Asher Y. and Brewis, Alexandra and Wutich, Amber and Jepson, Wendy and Staddon, Chad and Stoler, Justin and Young, Sera L.},
