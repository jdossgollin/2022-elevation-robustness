@article{vanderwiel_shortfall:2019,
  title = {Meteorological Conditions Leading to Extreme Low Variable Renewable Energy Production and Extreme High Energy Shortfall},
  shorttitle = {Shortfall},
  author = {family=Wiel, given=K., prefix=van der, useprefix=true and Stoop, L. P. and family=Zuijlen, given=B. R. H., prefix=van, useprefix=true and Blackport, R. and family=Broek, given=M. A., prefix=van den, useprefix=true and Selten, F. M.},
