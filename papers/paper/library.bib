@article{quinn_multireservoir:2018,
  title = {Exploring How Changing Monsoonal Dynamics and Human Pressures Challenge Multireservoir Management for Flood Protection, Hydropower Production, and Agricultural Water Supply},
  shorttitle = {Multireservoir},
  author = {Quinn, J. D. and Reed, P. M. and Giuliani, M. and Castelletti, A. and Oyler, J. W. and Nicholas, R. E.},
