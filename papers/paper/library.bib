  title = {New Insights into {{US}} Flood Vulnerability Revealed from Flood Insurance Big Data},
  shorttitle = {Vulnerability},
  author = {Wing, Oliver E. J. and Pinter, Nicholas and Bates, Paul D. and Kousky, Carolyn},
  date = {2020-03-19},
