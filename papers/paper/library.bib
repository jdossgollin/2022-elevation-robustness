@article{jehn_parameter:2019,
  title = {Trade-Offs between Parameter Constraints and Model Realism: A Case Study},
  shorttitle = {Parameter},
  author = {Jehn, Florian U. and Chamorro, Alejandro and Houska, Tobias and Breuer, Lutz},
  date = {2019-07-24},
  journaltitle = {Scientific Reports},
