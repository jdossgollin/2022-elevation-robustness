  author = {Paprotny, Dominik and Vousdoukas, Michalis I. and Morales-N\'apoles, Oswaldo and Jonkman, Sebastiaan N. and Feyen, Luc},
  date = {2018-04-19},
  journaltitle = {Hydrology and Earth System Sciences Discussions},
