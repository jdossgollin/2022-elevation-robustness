  title = {Rainwater {{Harvesting}} for {{Drinking Water Production}}: {{A Sustainable}} and {{Cost-Effective Solution}} in {{The Netherlands}}?},
  shorttitle = {Rwh Netherlands},
  author = {Hofman-Caris, Roberta and Bertelkamp, Cheryl and family=Waal, given=Luuk, prefix=de, useprefix=true and family=Brand, given=Tessa, prefix=van den, useprefix=true and Hofman, Jan and family=Aa, given=Ren\'e, prefix=van der, useprefix=true and family=Hoek, given=Jan Peter, prefix=van der, useprefix=true},
