@article{cioffi_agropontino:2017,
  title = {Large Scale Climate and Rainfall Seasonality in a {{Mediterranean Area}}: {{Insights}} from a Non-Homogeneous {{Markov}} Model Applied to the {{Agro-Pontino}} Plain},
  shorttitle = {Agropontino},
  author = {Cioffi, Francesco and Conticello, Federico and Lall, Upmanu and Marotta, Lucia and Telesca, Vito},
  date = {2017},
  journaltitle = {Hydrological Processes},
