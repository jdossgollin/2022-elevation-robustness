  author = {Daher, Bassel and Lee, Sang-Hyun and Kaushik, Vishakha and Blake, John and Askariyeh, Mohammad H. and Shafiezadeh, Hamid and Zamaripa, Sonia and Mohtar, Rabi H.},
  date = {2019-01-10},
