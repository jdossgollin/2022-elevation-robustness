@article{jamali_rwhflood:2020,
  title = {Rainwater Harvesting for Urban Flood Management \textendash{} {{An}} Integrated Modelling Framework},
  shorttitle = {Rwh Flood},
  author = {Jamali, Behzad and Bach, Peter M. and Deletic, Ana},
  date = {2020-03-15},
  journaltitle = {Water Research},
