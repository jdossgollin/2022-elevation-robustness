@article{srinivasan:2017,
  title = {A Dynamic Framework for Water Security},
  author = {Srinivasan, V. and Konar, M. and Sivapalan, M.},
  date = {2017-07-01},
  journaltitle = {Water Security},
  shortjournal = {Water Security},
