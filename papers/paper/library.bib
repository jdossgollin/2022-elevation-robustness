@article{munoz_datafusion:2021,
  title = {From Local to Regional Compound Flood Mapping with Deep Learning and Data Fusion Techniques},
  shorttitle = {Data Fusion},
  author = {Mu\~noz, David F. and Mu\~noz, Paul and Moftakhari, Hamed and Moradkhani, Hamid},
  date = {2021-08},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
