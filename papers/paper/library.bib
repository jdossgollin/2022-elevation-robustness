  author = {Palmer, M. D. and Gregory, J. M. and Bagge, M. and Calvert, D. and Hagedoorn, J. M. and Howard, T. and Klemann, V. and Lowe, J. A. and Roberts, C. D. and Slangen, A. B. A. and Spada, G.},
  date = {2020},
  journaltitle = {Earth's Future},
  volume = {8},
