@article{karl_relating:1990,
  title = {A {{Method}} of {{Relating General Circulation Model Simulated Climate}} to the {{Observed Local Climate}}. {{Part I}}: {{Seasonal Statistics}}},
  shorttitle = {Relating},
  author = {Karl, Thomas R. and Wang, Wei-Chyung and Schlesinger, Michael E. and Knight, Richard W. and Portman, David},
  date = {1990-10-01},
  journaltitle = {Journal of Climate},
