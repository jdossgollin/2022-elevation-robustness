  title = {Adaptation to Uncertain Sea-Level Rise: How Uncertainty in {{Antarctic}} Mass-Loss Impacts the Coastal Adaptation Strategy of the {{Netherlands}}},
  shorttitle = {Antarctic},
  author = {Haasnoot, M. and Kwadijk, J. and family=Alphen, given=J., prefix=van, useprefix=false and Bars, D. Le and family=Hurk, given=B., prefix=van den, useprefix=false and Diermanse, F. and family=Spek, given=A., prefix=van der, useprefix=false and Essink, G. Oude and Delsman, J. and Mens, M.},
