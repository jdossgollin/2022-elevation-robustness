@unpublished{ouazad_mortgage:2020,
  title = {Coastal Flood Risk in the Mortgage Market: Storm Surge Models' Predictions vs. Flood Insurance Maps},
  shorttitle = {Mortgage},
  author = {Ouazad, Amine},
  date = {2020-06-09},
  eprint = {2006.02977},
  eprinttype = {arxiv},
