@article{donner_cm3:2011,
  title = {The Dynamical Core, Physical Parameterizations, and Basic Simulation Characteristics of the Atmospheric Component {{AM3}} of the {{GFDL}} Global Coupled Model {{CM3}}},
  shorttitle = {Cm3},
  author = {Donner, Leo J. and Wyman, Bruce L. and Hemler, Richard S. and Horowitz, Larry W. and Ming, Yi and Zhao, Ming and Golaz, Jean-Christophe and Ginoux, Paul and Lin, S.-J. and Schwarzkopf, M. Daniel and Austin, John and Alaka, Ghassan and Cooke, William F. and Delworth, Thomas L. and Freidenreich, Stuart M. and Gordon, C. T. and Griffies, Stephen M. and Held, Isaac M. and Hurlin, William J. and Klein, Stephen A. and Knutson, Thomas R. and Langenhorst, Amy R. and Lee, Hyun-Chul and Lin, Yanluan and Magi, Brian I. and Malyshev, Sergey L. and Milly, P. C. D. and Naik, Vaishali and Nath, Mary J. and Pincus, Robert and Ploshay, Jeffrey J. and Ramaswamy, V. and Seman, Charles J. and Shevliakova, Elena and Sirutis, Joseph J. and Stern, William F. and Stouffer, Ronald J. and Wilson, R. John and Winton, Michael and Wittenberg, Andrew T. and Zeng, Fanrong},
  date = {2011-03-02},
  journaltitle = {Journal of Climate},
  shortjournal = {J. Climate},
  volume = {24},
  number = {13},
  pages = {3484--3519},
  issn = {0894-8755},
  doi = {10.1175/2011jcli3955.1},
  url = {http://journals.ametsoc.org/doi/10.1175/2011JCLI3955.1},
  urldate = {2020-02-18},
  abstract = {The Geophysical Fluid Dynamics Laboratory (GFDL) has developed a coupled general circulation model (CM3) for the atmosphere, oceans, land, and sea ice. The goal of CM3 is to address emerging issues in climate change, including aerosol\textendash cloud interactions, chemistry\textendash climate interactions, and coupling between the troposphere and stratosphere. The model is also designed to serve as the physical system component of earth system models and models for decadal prediction in the near-term future\textemdash for example, through improved simulations in tropical land precipitation relative to earlier-generation GFDL models. This paper describes the dynamical core, physical parameterizations, and basic simulation characteristics of the atmospheric component (AM3) of this model. Relative to GFDL AM2, AM3 includes new treatments of deep and shallow cumulus convection, cloud droplet activation by aerosols, subgrid variability of stratiform vertical velocities for droplet activation, and atmospheric chemistry driven by emissions with advective, convective, and turbulent transport. AM3 employs a cubed-sphere implementation of a finite-volume dynamical core and is coupled to LM3, a new land model with ecosystem dynamics and hydrology. Its horizontal resolution is approximately 200 km, and its vertical resolution ranges approximately from 70 m near the earth's surface to 1 to 1.5 km near the tropopause and 3 to 4 km in much of the stratosphere. Most basic circulation features in AM3 are simulated as realistically, or more so, as in AM2. In particular, dry biases have been reduced over South America. In coupled mode, the simulation of Arctic sea ice concentration has improved. AM3 aerosol optical depths, scattering properties, and surface clear-sky downward shortwave radiation are more realistic than in AM2. The simulation of marine stratocumulus decks remains problematic, as in AM2. The most intense 0.2\% of precipitation rates occur less frequently in AM3 than observed. The last two decades of the twentieth century warm in CM3 by 0.32\textdegree C relative to 1881\textendash 1920. The Climate Research Unit (CRU) and Goddard Institute for Space Studies analyses of observations show warming of 0.56\textdegree{} and 0.52\textdegree C, respectively, over this period. CM3 includes anthropogenic cooling by aerosol\textendash cloud interactions, and its warming by the late twentieth century is somewhat less realistic than in CM2.1, which warmed 0.66\textdegree C but did not include aerosol\textendash cloud interactions. The improved simulation of the direct aerosol effect (apparent in surface clear-sky downward radiation) in CM3 evidently acts in concert with its simulation of cloud\textendash aerosol interactions to limit greenhouse gas warming.}
}
