@article{santella_hazardous:2010,
  title = {Petroleum and Hazardous Material Releases from Industrial Facilities Associated with {{Hurricane Katrina}}},
  shorttitle = {Hazardous},
  author = {Santella, Nicholas and Steinberg, Laura J. and Sengul, Hatice},
  date = {2010},
