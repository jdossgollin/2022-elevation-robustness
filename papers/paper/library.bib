  shorttitle = {Colored Noise and Computational Inference in Neurophysiological ({{fMRI}}) Time Series Analysis},
  author = {Bullmore, Ed and Long, Chris and Suckling, John and Fadili, Jalal and Calvert, Gemma and Zelaya, Fernando and Carpenter, T. Adrian and Brammer, Mick},
  date = {2001},
  journaltitle = {Human Brain Mapping},
