  title = {The Third Route: Using Extreme Decentralization to Create Resilient Urban Water Systems},
  shorttitle = {Decentralization},
  author = {Rabaey, Korneel and Vandekerckhove, Tom and family=Walle, given=Arjen Van, prefix=de, useprefix=true and Sedlak, David L.},
