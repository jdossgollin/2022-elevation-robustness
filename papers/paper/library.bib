@article{jamali_ml2d:2021,
  title = {Machine Learning for Accelerating {{2D}} Flood Models: {{Potential}} and Challenges},
  shorttitle = {{{ML 2D}}},
  author = {Jamali, Behzad and Haghighat, Ehsan and Ignjatovic, Aleksandar and Leit\~ao, Jo\~ao P. and Deletic, Ana},
