  shorttitle = {Meta Analysis},
  author = {Bell, Colin D. and Wolfand, Jordyn M. and Panos, Chelsea L. and Bhaskar, Aditi S. and Gilliom, Ryan L. and Hogue, Terri S. and Hopkins, Kristina G. and Jefferson, Anne J.},
