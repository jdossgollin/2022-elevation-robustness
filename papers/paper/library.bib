@article{wong_antarctic:2017,
  title = {Impacts of {{Antarctic}} Fast Dynamics on Sea-Level Projections and Coastal Flood Defense},
  shorttitle = {Antarctic},
  author = {Wong, Tony E. and Bakker, Alexander M. R. and Keller, Klaus},
  date = {2017-09-01},
