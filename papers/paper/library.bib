@article{ciullo_operationalizing:2020,
  title = {Efficient or Fair? {{Operationalizing}} Ethical Principles in Flood Risk Management: {{A}} Case Study on the {{Dutch-German Rhine}}},
  shorttitle = {Operationalizing},
