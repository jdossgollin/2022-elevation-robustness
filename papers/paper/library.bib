  title = {Direct Policy Search for Robust Multi-Objective Management of Deeply Uncertain Socio-Ecological Tipping Points},
  shorttitle = {Dps},
  author = {Quinn, Julianne D. and Reed, Patrick M. and Keller, Klaus},
  date = {2017-06-01},
