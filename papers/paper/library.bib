@article{blum_impervious:2020,
  title = {Causal {{Effect}} of {{Impervious Cover}} on {{Annual Flood Magnitude}} for the {{United States}}},
  shorttitle = {Impervious},
  author = {Blum, Annalise G. and Ferraro, Paul J. and Archfield, Stacey A. and Ryberg, Karen R.},
  date = {2020},
  journaltitle = {Geophysical Research Letters},
  volume = {47},
  number = {5},
