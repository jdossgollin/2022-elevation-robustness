@unpublished{gelman_workflow:2020,
  title = {Bayesian {{Workflow}}},
  shorttitle = {Workflow},
  author = {Gelman, Andrew and Vehtari, Aki and Simpson, Daniel and Margossian, Charles C. and Carpenter, Bob and Yao, Yuling and Kennedy, Lauren and Gabry, Jonah and B\"urkner, Paul-Christian and Modr\'ak, Martin},
  date = {2020-11-03},
