@article{weijs_information:2020,
  title = {Debates: Does Information Theory Provide a New Paradigm for {{Earth Science}}? {{Sharper}} Predictions Using {{Occam}}'s {{Digital Razor}}},
  shorttitle = {Information},
  author = {Weijs, Steven V. and Ruddell, Benjamin L.},
  date = {2020},
  journaltitle = {Water Resources Research},
