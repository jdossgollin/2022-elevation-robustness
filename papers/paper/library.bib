@article{gori_compound:2020,
  title = {Assessing Compound Flooding from Landfalling Tropical Cyclones on the {{North Carolina}} Coast},
  shorttitle = {Compound},
  author = {Gori, Avantika and Lin, Ning and Smith, James},
  date = {2020},
  journaltitle = {Water Resources Research},
  volume = {56},
  number = {4},
  pages = {e2019WR026788},
