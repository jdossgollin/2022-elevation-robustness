@article{reed_interrogating:2019,
  title = {Interrogating Uncertainty in Energy Forecasts: The Case of the Shale Gas Boom},
  shorttitle = {Interrogating},
  author = {Reed, Adam and Ericson, Sean and Bazilian, Morgan and Logan, Jeffrey and Doran, Kevin and Nelder, Chris},
  date = {2019-09-05},
