@article{huntingford:2019,
  title = {Assessing Changes in Risk of Amplified Planetary Waves in a Warming World},
  author = {Huntingford, Chris and Mitchell, Dann and Kornhuber, Kai and Coumou, Dim and Osprey, Scott and Allen, Myles},
