@article{duchez_atlantic:2016,
  title = {Drivers of Exceptionally Cold {{North Atlantic Ocean}} Temperatures and Their Link to the 2015 {{European}} Heat Wave},
  shorttitle = {Atlantic},
  author = {Duchez, Aur\'elie and Frajka-Williams, Eleanor and Josey, Simon A. and Evans, Dafydd G. and Grist, Jeremy P. and Marsh, Robert and McCarthy, Gerard D. and Sinha, Bablu and Berry, David I. and Hirschi, Jo\"el J.-M.},
  date = {2016-07},
