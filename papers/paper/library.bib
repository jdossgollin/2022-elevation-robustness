@article{kass_prior:1996,
  title = {The Selection of Prior Distributions by Formal Rules},
  shorttitle = {Prior},
  author = {Kass, Robert E. and Wasserman, Larry},
  date = {1996},
  journaltitle = {Journal of the American Statistical Association},
  volume = {91},
