@article{chattopadhyay_clustered:2020,
  title = {Predicting Clustered Weather Patterns: {{A}} Test Case for Applications of Convolutional Neural Netwcorks to Spatio-Temporal Climate Data},
  shorttitle = {Clustered},
  author = {Chattopadhyay, Ashesh and Hassanzadeh, Pedram and Pasha, Saba},
