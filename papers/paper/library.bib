  title = {The Roles of Climate Change and Climate Variability in the 2017 {{Atlantic}} Hurricane Season},
  shorttitle = {Hurricane},
  author = {Lim, Young-Kwon and Schubert, Siegfried D. and Kovach, Robin and Molod, Andrea M. and Pawson, Steven},
  date = {2018-11-01},
