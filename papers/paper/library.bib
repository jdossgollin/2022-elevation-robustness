@article{DossGollin:2019,
  title = {Robust Adaptation to Multiscale Climate Variability},
  author = {Doss-Gollin, James and Farnham, David J. and Steinschneider, Scott and Lall, Upmanu},
  date = {2019-06-07},
  journaltitle = {Earth's Future},
  volume = {7},
