@article{wong_brick0.2:2017,
  title = {{{BRICK}} v0.2, a Simple, Accessible, and Transparent Model Framework for Climate and Regional Sea-Level Projections},
  shorttitle = {Brick0.2},
  author = {Wong, Tony E. and Bakker, Alexander M. R. and Ruckert, Kelsey and Applegate, Patrick and Slangen, Aim\'ee B. A. and Keller, Klaus},
