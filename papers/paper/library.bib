  author = {Ebad Sichani, Majid and Anarde, Katherine A. and Capshaw, Kendall M. and Padgett, Jamie E. and Meidl, Rachel A. and Hassanzadeh, Pedram and Loch-Temzelides, Ted P. and Bedient, Philip B.},
  date = {2020},
  journaltitle = {Frontiers in Built Environment},
