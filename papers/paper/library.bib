@article{saxena_objectivereduction:2013,
  title = {Objective Reduction in Many-Objective Optimization: Linear and Nonlinear Algorithms},
  shorttitle = {Objective {{Reduction}}},
  author = {Saxena, D. K. and Duro, J. A. and Tiwari, A. and Deb, K. and Zhang, Q.},
  date = {2013-02},
