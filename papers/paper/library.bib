@article{block_streamflow:2009,
  title = {A Streamflow Forecasting Framework Using Multiple Climate and Hydrological Models},
  shorttitle = {Streamflow},
  author = {Block, Paul J. and Filho, Francisco Assis Souza and Sun, Liqiang and Kwon, Hyun-Han},
  date = {2009},
