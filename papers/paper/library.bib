@article{savelli_capetown:2021,
  title = {Don't Blame the Rain: {{Social}} Power and the 2015\textendash 2017 Drought in {{Cape Town}}},
  shorttitle = {Cape Town},
  author = {Savelli, Elisa and Rusca, Maria and Cloke, Hannah and Di Baldassarre, Giuliano},
  date = {2021-03-01},
