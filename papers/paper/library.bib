@article{chen_drizzlebias:2021,
  title = {The {{Convective-To-Total Precipitation Ratio}} and the ``{{Drizzling}}'' {{Bias}} in {{Climate Models}}},
  shorttitle = {Drizzle Bias},
  author = {Chen, Di and Dai, Aiguo and Hall, Alex},
  date = {2021},
