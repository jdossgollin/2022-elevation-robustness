  author = {Bloemen, Pieter and Reeder, Tim and Zevenbergen, Chris and Rijke, Jeroen and Kingsborough, Ashley},
  date = {2018},
  journaltitle = {Mitigation and Adaptation Strategies for Global Change},
  shortjournal = {Mitig Adapt Strateg Glob Chang},
  volume = {23},
